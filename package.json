{
  "name": "videu-frontend",
  "version": "0.1.0",
  "description": "The web frontend for the videu platform",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development gulp serve",
    "build": "NODED_ENV=production tsc && polymer build --auto-base-path && gulp prpl-server",
    "build:static": "NODE_ENV=production tsc && polymer build",
    "serve": "NODE_ENV=production prpl-server --root server/build",
    "serve:static": "NODE_ENV=production polymer serve build/es6-bundled",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:integration": "tsc; mocha test/integration --timeout=10000",
    "test:unit": "tsc; polymer test --module-resolution=node --npm",
    "test:sauce": "tsc; polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/videu/frontend.git"
  },
  "keywords": [
    "videu",
    "freetube",
    "polymer",
    "lit-element",
    "lit-html",
    "video",
    "pwa"
  ],
  "author": "Felix Kopp <sandtler@sandtler.club>",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/videu/frontend/issues"
  },
  "homepage": "https://github.com/videu/frontend#readme",
  "dependencies": {
    "@polymer/polymer": "^3.3.1",
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "lit-element": "^2.2.1",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.5.1",
    "chai": "^4.2.0",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^7.0.1",
    "pixelmatch": "^5.1.0",
    "polymer-cli": "^1.9.11",
    "prpl-server": "^1.4.0",
    "puppeteer": "^2.1.1",
    "typescript": "^3.7.5",
    "wct-mocha": "^1.0.1"
  }
}
