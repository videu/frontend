{
    "entrypoint": "index.html",
    "shell": "tsbuild/components/vid-eu.js",
    "sources": [
        "img/**/*"
    ],
    "extraDependencies": [
        "manifest.json",
        "node_modules/@webcomponents/webcomponentsjs/**",
        "push-manifest.json"
    ],
    "builds": [
        {
            "name": "esm-bundled",
            "browserCapabilities": [
                "es2015",
                "modules"
            ],
            "js": {
                "minify": true
            },
            "css": {
                "minify": true
            },
            "html": {
                "minify": true
            },
            "bundle": {
                "sourcemaps": true,
                "stripComments": true
            },
            "addServiceWorker": true
        },
        {
            "name": "es6-bundled",
            "browserCapabilities": [
                "es2015"
            ],
            "js": {
                "compile": "es2015",
                "minify": true,
                "transformModulesToAmd": true
            },
            "css": {
                "minify": true
            },
            "html": {
                "minify": true
            },
            "bundle": {
                "sourcemaps": true,
                "stripComments": true
            },
            "addServiceWorker": true
        }
    ],
    "moduleResolution": "node",
    "npm": true
}
